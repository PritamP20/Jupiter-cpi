{
  "license": "ISC",
  "type": "module",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "test": "anchor test",
    "test:devnet": "anchor test tests/devnet-swap.ts --provider.cluster devnet",
    "test:mainnet": "anchor test tests/mainnet-swap.ts --provider.cluster mainnet",
    "test:original": "anchor test tests/cpi-swap-program.ts --provider.cluster devnet",
    "deploy-devnet": "solana program deploy target/deploy/cpi_swap_program.so   --program-id target/deploy/cpi_swap_program-keypair.json   -C ~/.config/solana/cli/config.yml   --use-rpc --with-compute-unit-price 10",
    "deploy-mainnet": "solana program deploy target/deploy/cpi_swap_program.so --program-id target/deploy/cpi_swap_program-keypair.json -C ~/.config/solana/cli/mainnet.yml --use-rpc --with-compute-unit-price 50",
    "resume-deploy-mainnet": "solana program deploy --buffer buffer-keypair.json --program-id target/deploy/cpi_swap_program-keypair.json -C ~/.config/solana/cli/mainnet.yml --use-rpc --with-compute-unit-price 50"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.0",
    "bn.js": "^5.2.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^20.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
